ON d24_empresa
FOR EACH ROW
BEGIN
    DECLARE idEmpresa BIGINT(20);
    DECLARE amostragem1 VARCHAR(100);
    DECLARE amostragem2 VARCHAR(100);
    DECLARE amostragem3 VARCHAR(100);
    DECLARE amostragem4 VARCHAR(100);
    DECLARE amostragem5 VARCHAR(100);
    DECLARE amostragem6 VARCHAR(100);
    DECLARE amostragem7 VARCHAR(100);
    DECLARE amostragem8 VARCHAR(100);
    DECLARE amostragem9 VARCHAR(100);
