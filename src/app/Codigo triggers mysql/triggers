    select d06_nmamostragem INTO amostragem1 from d06_amostragem_m where d06_cdamostragem =1;
    select d06_nmamostragem INTO amostragem2 from d06_amostragem_m where d06_cdamostragem =2;
    select d06_nmamostragem INTO amostragem3 from d06_amostragem_m where d06_cdamostragem =3;
    select d06_nmamostragem INTO amostragem4 from d06_amostragem_m where d06_cdamostragem =4;
    select d06_nmamostragem INTO amostragem5 from d06_amostragem_m where d06_cdamostragem =5;
    select d06_nmamostragem INTO amostragem6 from d06_amostragem_m where d06_cdamostragem =6;
    select d06_nmamostragem INTO amostragem7 from d06_amostragem_m where d06_cdamostragem =7;
     select d06_nmamostragem INTO amostragem8 from d06_amostragem_m where d06_cdamostragem =8;
    select d06_nmamostragem INTO amostragem9 from d06_amostragem_m where d06_cdamostragem =9;
    
    INSERT INTO `mopapi`.`d06_amostragem_m`
(`d06_cdempresa`,`d06_nmamostragem`) VALUES(idEmpresa, amostragem1);
INSERT INTO `mopapi`.`d06_amostragem_m`
