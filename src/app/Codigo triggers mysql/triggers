(`d06_cdempresa`,`d06_nmamostragem`) VALUES(idEmpresa, amostragem9);
END$


-- Trigger Frequencia


DELIMITER $
CREATE TRIGGER Tgr_d24_empresa_frequencia_Insert AFTER INSERT
ON d24_empresa
FOR EACH ROW
BEGIN
    DECLARE idEmpresa BIGINT(20);
    DECLARE frequencia1 VARCHAR(100);
    DECLARE frequencia2 VARCHAR(100);
    DECLARE frequencia3 VARCHAR(100);
    DECLARE frequencia4 VARCHAR(100);
    DECLARE frequencia5 VARCHAR(100);
    DECLARE frequencia6 VARCHAR(100);
    DECLARE frequencia7 VARCHAR(100);
    DECLARE frequencia8 VARCHAR(100);
    DECLARE frequencia9 VARCHAR(100);
	select MAX(d24_cdempresa) INTO idEmpresa from d24_empresa;
    select d08_nmfrequencia INTO frequencia1 from d08_frequencia_m where d08_cdfrequencia =1;
    select d08_nmfrequencia INTO frequencia2 from d08_frequencia_m where d08_cdfrequencia =2;
    select d08_nmfrequencia INTO frequencia3 from d08_frequencia_m where d08_cdfrequencia =3;
    select d08_nmfrequencia INTO frequencia4 from d08_frequencia_m where d08_cdfrequencia =4;
    select d08_nmfrequencia INTO frequencia5 from d08_frequencia_m where d08_cdfrequencia =5;
    select d08_nmfrequencia INTO frequencia6 from d08_frequencia_m where d08_cdfrequencia =6;
    select d08_nmfrequencia INTO frequencia7 from d08_frequencia_m where d08_cdfrequencia =7;
     select d08_nmfrequencia INTO frequencia8 from d08_frequencia_m where d08_cdfrequencia =8;
    
    INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia1);
INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia2);
INSERT INTO `mopapi`.`d08_frequencia_m`
