--- Trigger tipo de metodo

DELIMITER $
CREATE TRIGGER Tgr_d24_empresa_tipodemetodoInsert AFTER INSERT
ON d24_empresa
FOR EACH ROW
BEGIN
    DECLARE idEmpresa BIGINT(20);
    DECLARE tipometodo1 VARCHAR(100);
    DECLARE tipometodo2 VARCHAR(100);
    DECLARE tipometodo3 VARCHAR(100);
    DECLARE tipometodo4 VARCHAR(100);
    DECLARE tipometodo5 VARCHAR(100);
    DECLARE tipometodo6 VARCHAR(100);
    DECLARE tipometodo7 VARCHAR(100);
	select MAX(d24_cdempresa) INTO idEmpresa from d24_empresa;
    select d07_nmtipometodo INTO tipometodo1 from d07_tipo_metodo_m where d07_cdtipometodo =1;
    select d07_nmtipometodo INTO tipometodo2 from d07_tipo_metodo_m where d07_cdtipometodo =2;
    select d07_nmtipometodo INTO tipometodo3 from d07_tipo_metodo_m where d07_cdtipometodo =3;
    select d07_nmtipometodo INTO tipometodo4 from d07_tipo_metodo_m where d07_cdtipometodo =4;
    select d07_nmtipometodo INTO tipometodo5 from d07_tipo_metodo_m where d07_cdtipometodo =5;
    select d07_nmtipometodo INTO tipometodo6 from d07_tipo_metodo_m where d07_cdtipometodo =6;
    select d07_nmtipometodo INTO tipometodo7 from d07_tipo_metodo_m where d07_cdtipometodo =7;
    
    INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo1);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo2);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo3);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo4);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo5);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo6);
INSERT INTO `mopapi`.`d07_tipo_metodo_m`
(`d07_cdempresa`,`d07_nmtipometodo`) VALUES(idEmpresa, tipometodo7);
END$


