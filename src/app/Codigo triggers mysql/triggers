INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia5);
INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia6);
INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia7);
INSERT INTO `mopapi`.`d08_frequencia_m`
(`d08_cdempresa`,`d08_nmfrequencia`) VALUES(idEmpresa, frequencia8);
END$


-- Trigger Materiais


DELIMITER $
CREATE TRIGGER Tgr_d24_empresa_material_Insert AFTER INSERT
ON d24_empresa
FOR EACH ROW
BEGIN
    DECLARE idEmpresa BIGINT(20);
    DECLARE material1 VARCHAR(100);
    DECLARE material2 VARCHAR(100);
    DECLARE material3 VARCHAR(100);
    DECLARE material4 VARCHAR(100);
    DECLARE material5 VARCHAR(100);
    DECLARE material6 VARCHAR(100);
    DECLARE material7 VARCHAR(100);
    DECLARE material8 VARCHAR(100);
    DECLARE material9 VARCHAR(100);
    DECLARE material10 VARCHAR(100);
    DECLARE material11 VARCHAR(100);
    DECLARE material12 VARCHAR(100);
    DECLARE material13 VARCHAR(100);
    DECLARE material14 VARCHAR(100);
    DECLARE material15 VARCHAR(100);
    DECLARE material16 VARCHAR(100);
    DECLARE material17 VARCHAR(100);
    DECLARE material18 VARCHAR(100);
    DECLARE material19 VARCHAR(100);
    DECLARE material20 VARCHAR(100);
    DECLARE material21 VARCHAR(100);
    DECLARE material22 VARCHAR(100);
    DECLARE material23 VARCHAR(100);
	select MAX(d24_cdempresa) INTO idEmpresa from d24_empresa;
    select d09_nmmaterial INTO material1 from d09_material_m where d09_cdmaterial =1;
    select d09_nmmaterial INTO material2 from d09_material_m where d09_cdmaterial =2;
    select d09_nmmaterial INTO material3 from d09_material_m where d09_cdmaterial =3;
    select d09_nmmaterial INTO material4 from d09_material_m where d09_cdmaterial =4;
    select d09_nmmaterial INTO material5 from d09_material_m where d09_cdmaterial =5;
    select d09_nmmaterial INTO material6 from d09_material_m where d09_cdmaterial =6;
    select d09_nmmaterial INTO material7 from d09_material_m where d09_cdmaterial =7;
	select d09_nmmaterial INTO material8 from d09_material_m where d09_cdmaterial =8;
    select d09_nmmaterial INTO material9 from d09_material_m where d09_cdmaterial =9;
    select d09_nmmaterial INTO material10 from d09_material_m where d09_cdmaterial =10;
    select d09_nmmaterial INTO material11 from d09_material_m where d09_cdmaterial =11;
    select d09_nmmaterial INTO material12 from d09_material_m where d09_cdmaterial =12;
    select d09_nmmaterial INTO material13 from d09_material_m where d09_cdmaterial =13;
    select d09_nmmaterial INTO material14 from d09_material_m where d09_cdmaterial =14;
    select d09_nmmaterial INTO material15 from d09_material_m where d09_cdmaterial =15;
	select d09_nmmaterial INTO material16 from d09_material_m where d09_cdmaterial =16;
    select d09_nmmaterial INTO material17 from d09_material_m where d09_cdmaterial =17;
    select d09_nmmaterial INTO material18 from d09_material_m where d09_cdmaterial =18;
    select d09_nmmaterial INTO material19 from d09_material_m where d09_cdmaterial =19;
    select d09_nmmaterial INTO material20 from d09_material_m where d09_cdmaterial =20;
    select d09_nmmaterial INTO material21 from d09_material_m where d09_cdmaterial =21;
    select d09_nmmaterial INTO material22 from d09_material_m where d09_cdmaterial =22;
	select d09_nmmaterial INTO material23 from d09_material_m where d09_cdmaterial =23;
    
    
    INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material1);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material2);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material3);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material4);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material5);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material6);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material7);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material8);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material9);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material10);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material11);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material12);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material13);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material14);
