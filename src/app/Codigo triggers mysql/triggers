INSERT INTO `mopapi`.`d06_amostragem_m`
(`d06_cdempresa`,`d06_nmamostragem`) VALUES(idEmpresa, amostragem5);
INSERT INTO `mopapi`.`d06_amostragem_m`
(`d06_cdempresa`,`d06_nmamostragem`) VALUES(idEmpresa, amostragem6);
INSERT INTO `mopapi`.`d06_amostragem_m`
(`d06_cdempresa`,`d06_nmamostragem`) VALUES(idEmpresa, amostragem7);
INSERT INTO `mopapi`.`d06_amostragem_m`
