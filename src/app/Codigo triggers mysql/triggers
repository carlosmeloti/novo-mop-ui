(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material15);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material16);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material17);
INSERT INTO `mopapi`.`d09_material_m`
(`d09_cdempresa`,`d09_nmmaterial`) VALUES(idEmpresa, material18);
